#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int sum = 0, total_sum = 0, count = 0;

    while (N != 0) {
        int digit = N % 10;
        total_sum += digit;
        count++;
        N = N / 10;
    }

    printf("%d\n", total_sum * (count - 1));

    return 0;
}

